<% @submissions.each do |submission| %>
  <dl class="dl-horizontal">
    <dt>ID</dt>
    <dd><%= submission['submissionId'] %></dd>
    <dt>Date</dt>
    <dd><%= local_timestamp(submission['submissionDate']) %></dd>
    <dt>Sample</dt>
    <dd><%= submission['request']['entityName'] %></dd>
  </dl>
<% end %>

